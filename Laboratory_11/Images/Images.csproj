<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net8.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UseWPF>true</UseWPF>
    <ApplicationIcon>ic_app.ico</ApplicationIcon>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="ImagesSrc\left.png" />
    <None Remove="ImagesSrc\right.png" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="ic_app.ico" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="ImagesSrc\left.png">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Resource>
    <Resource Include="ImagesSrc\right.png">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Resource>
  </ItemGroup>

</Project>
